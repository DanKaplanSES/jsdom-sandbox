{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jasmine": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "State": "writable",
    "setup": "writable"
  },
  "plugins": [],
  "ignorePatterns": [],
  "rules": {
    "quotes": [
      "off"
    ],
    "no-plusplus": [
      "off"
    ]
  }
}